# Uncomment the next line to define a global platform for your project
platform :ios, '11.4'

target 'Qast' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  pod 'SwiftLint'
  pod 'SnapKit', '~>5.0.0'
  pod 'Alamofire', '~>4.8.1'
  pod 'Mapbox-iOS-SDK', '~> 5.1'
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'
  pod 'GeoFire', '~> 3.0'
  pod 'lottie-ios', '~> 3.1.1'
  pod 'SDWebImage', '~> 5.0'
  
  # Bose AR SDK
  pod 'BoseWearable', :path => 'BoseWearable/'
  pod 'BLECore', :path => 'BoseWearable/'
  pod 'Logging', :path => 'BoseWearable/'
  
  target 'QastTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
